@page

<h1>Hello from my first web page</h1>

@for (int i = 1; i <= 20; i++)
{
    <b>@i</b>
}

<h3>Ex1. Even numbers from 1 to 100</h3>
@for (int i = 1; i <= 100; i++)
{
    if (i % 2 == 0)
    {
        <b>@i</b>
    }
}

<h3>Ex2. Numbers containing 9 in it from 1 to 100</h3>
@for (int i = 1; i <= 100; i++)
{
    if (i.ToString().Contains("9"))
    {
        <b>@i</b>
    }
}
